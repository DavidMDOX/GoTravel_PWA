{
  "version": 2,
  "routes": [
    {
      "src": "^/sw.js$",
      "dest": "/sw.js"
    },
    {
      "src": "^/manifest.json$",
      "dest": "/manifest.json"
    },
    {
      "src": "^/app.js$",
      "dest": "/app.js"
    },
    {
      "src": "^/styles.css$",
      "dest": "/styles.css"
    },
    {
      "src": "^/icons/(.*)$",
      "dest": "/icons/$1"
    },
    {
      "src": "^(?!/api/).*$",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}